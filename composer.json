{
    "name": "netizens-gaurav/filament-onboarding",
    "description": "A comprehensive onboarding solution for Filament applications with support for both single-tenant and multi-tenant SaaS platforms",
    "keywords": [
        "netizens-gaurav",
        "laravel",
        "filament-onboarding"
    ],
    "homepage": "https://github.com/netizens-gaurav/filament-onboarding",
    "support": {
        "issues": "https://github.com/netizens-gaurav/filament-onboarding/issues",
        "source": "https://github.com/netizens-gaurav/filament-onboarding"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Gaurav Vaishnav",
            "email": "gaurav.v@netizens.email",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "filament/filament": "^3.0",
        "spatie/laravel-package-tools": "^1.15.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^7.9",
        "orchestra/testbench": "^8.0",
        "pestphp/pest": "^2.1",
        "pestphp/pest-plugin-arch": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0",
        "phpstan/phpstan": "^1.10",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Netizensgaurav\\FilamentOnboarding\\": "src/",
            "Netizensgaurav\\FilamentOnboarding\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Netizensgaurav\\FilamentOnboarding\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "pint": "./vendor/bin/pint",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Netizensgaurav\\FilamentOnboarding\\FilamentOnboardingServiceProvider"
            ],
            "aliases": {
                "FilamentOnboarding": "Netizensgaurav\\FilamentOnboarding\\Facades\\FilamentOnboarding"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
